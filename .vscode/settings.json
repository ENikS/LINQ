{
  "files.exclude": {
    "**/*.js": { "when": "$(basename).ts"},
    "test/*.d.ts": true,
    "lib/*.d.ts": true,
    "**/*.map": true,
    "**/*.njsproj": true,
    "**/*.git": true,
    "**/*.svn": true,
    "**/*.hg": true,
    "TestResults": true,
    "obj": true,
    "Obj": true,
    "bin": true,
    "Bin": true,
    ".vs": true,
    ".vscode": true,
    "node_modules": true,
    "**/*.DS_Store": true
  }    
}


